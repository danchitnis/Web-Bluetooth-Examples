{
  "name": "arduino-web-examples",
  "version": "0.0.1",
  "description": "Using the IMU 9-axis sensor to plot on webgl-plot",
  "main": "index.js",
  "dependencies": {
    "@types/web-bluetooth": "0.0.4"
  },
  "devDependencies": {
    "tslint": "^5.20.1",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "build1": "tsc BLE_LED/src/BLE_LED.ts --outFile BLE_LED/dist/BLE_LED.js --sourceMap true",
    "build2": "tsc BLE_Stream/src/BLE_Stream.ts --outFile BLE_Stream/dist/BLE_Stream.js --sourceMap true",
    "build": "npm run build1 && npm run build2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danchitnis/arduino-web-examples.git"
  },
